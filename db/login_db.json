{
    "db_pkg": "ih_login_db",
    "charset": "utf8",
    "tables":[
        {
            "name": "accounts",
            "primary_key": "account",
            "engine": "innodb",
            "fields": [
                {"name":"account", "type":"varchar(64) not null default ''"},
                {"name":"unique_id", "type":"varchar(64) not null default ''"},
                {"name":"password", "type":"varchar(32) not null default ''"},
                {"name":"register_time", "type":"int unsigned not null default 0"},
                {"name":"channel", "type":"varchar(16) not null default ''"},
                {"name":"token", "type":"varchar(64) not null default ''"},
                {"name":"last_get_account_player_list_time", "type":"int unsigned not null default 0"},
                {"name":"last_select_server_id", "type":"int unsigned not null default 0"},
                {"name":"last_select_ios_server_id", "type":"int unsigned not null default 0"},
                {"name":"bind_new_account", "type":"varchar(64) not null default ''"},
                {"name":"old_account", "type":"varchar(64) not null default ''"}
            ]
        },
        {
            "name": "ban_players",
            "primary_key": "unique_id",
            "engine": "innodb",
            "fields": [
                {"name":"unique_id", "type":"varchar(64) not null default ''"},
                {"name":"account", "type":"varchar(64) not null default ''"},
                {"name":"player_id", "type":"int unsigned not null default 0"},
                {"name":"start_time", "type":"int unsigned not null default 0"},
                {"name":"start_time_str", "type":"varchar(32) not null default ''"},
                {"name":"duration", "type":"int unsigned not null default 0"}
            ]
        },
        {
            "name": "accounts_aaid",
            "primary_key": "acc_aaid",
            "engine": "innodb",
            "fields": [
                {"name":"acc_aaid", "type":"varchar(256) not null default ''"},
                {"name":"account", "type":"varchar(64) not null default ''"},
                {"name":"aaid", "type":"varchar(64) not null default ''"}
            ]
        }
    ]
}